super + Return
  $HOME/.scripts/dmenu/launch-dmenu.sh
  
super + Home
  $HOME/.scripts/set-random-wallpaper.sh

XF86Audio{Raise,Lower}Volume
  $HOME/.scripts/{raise-,lower-}volume.sh

super + F1
  $HOME/.scripts/mute.sh

super + e
  urxvtc -title Files -e ranger

super + Shift + escape
  killall xinit

Print
  scrot "$(date +%c).png" -e 'mv "$f" ~/Pictures/Screenshots/ ; dunstify -t 1000 -i ~/Pictures/Screenshots/apple-camera.png "Screenshot Saved" '
